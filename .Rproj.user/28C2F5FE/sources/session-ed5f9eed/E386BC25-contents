#check similarity between different files/versions


#check Human-2019-Ximerakis-BaderLab-2017.xlsx and receptor_ligand_interactions_mitab_v1.0_April2017.xlsx
# Load the necessary packages
library(readxl)
library(VennDiagram)

# Read the Excel files
file1 <- read_excel("/Users/diandra/rlp_meta/data/input_files_for_db/Human-2019-Ximerakis-BaderLab-2017.xlsx")
file2 <- read_excel("/Users/diandra/rlp_meta/data/input_files_for_db/receptor_ligand_interactions_mitab_v1.0_April2017.xlsx")
# Assuming that file1 and file2 are your data frames

# Assuming that file1 and file2 are your data frames

# Extract the "AliasA" column from both files
aliasA_file1 <- file1$AliasA
aliasA_file2 <- file2$AliasA

# Check for identicality between the two columns
identicality <- aliasA_file1 == aliasA_file2

# Create a Venn diagram
library(VennDiagram)

# Create a new plot window
dev.new()

# Create the Venn diagram
venn.plot <- venn.diagram(
  x = list(
    "File 1" = sum(!identicality & aliasA_file1 != ""),
    "File 2" = sum(!identicality & aliasA_file2 != ""),
    "01" = sum(identicality & aliasA_file1 != "")
  ),
  filename = NULL,
  imagetype = "none"
)

# Draw the diagram in the new plot window
grid.draw(venn.plot)
